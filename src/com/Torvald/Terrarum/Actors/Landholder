public interface Landholder {

    ArrayList<Integer> getHouseDesignation();
    void setHouseDesignation(ArrayList<Integer> list);
    void addHouseTile(int x, int y);
    void removeHouseTile(int x, int y);
    void clearHouseDesignation();

}
